{
    "name": "doctrine/tester",
    "type": "library",
    "require": {
        "php": ">=5.3.2"
    },
    "recommend": {
        "doctrine/orm": "dev-master",
        "doctrine/data-fixtures": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Doctrine\\Common\\Tester": "src/",
            "Doctrine\\Tests\\Common\\Tester": "tests/"
        }
    },
    "target-dir": "Doctrine/Common/Tester"
}